<body>

	<!-- Header -->

	<% include header %>

		<!-- End Header -->

		<!--====== Login Page ======-->

		<section class="contact section-padding">

			<div class="section-head spac">
				<h3>Login</h3>
			</div>

			<div class="wrapper">
				<div id="formContent">
					<!-- Tabs Titles -->

					<% if (message.length> 0) { %>
						<div class="alert alert-danger col-sm-12">
							<%= message %>
						</div>
						<% } %>

							<!-- Login Form -->
							<form class="mb-10" method="POST" action="/login">
								<input style="width: 75%;" type="email" id="login" class="second" name="emailId"
									placeholder="Email Address" required>
								<input style="width: 75%;" type="password" id="password" class="third" name="password"
									placeholder="Password" required>
								<input type="submit" class="fourth" value="Log In">
							</form>
							<a class="forgot-password" href="/forgotpassword">Forgot Your Password?</a><br>
							<a class="forgot-password" href="/register">Create My Account</a>
				</div>
			</div>
		</section>
		<!--====== End Login Page ======-->

		<!-- footer -->

		<% include footer %>

			<!-- End Footer -->

</body>

