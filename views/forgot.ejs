	<!--[endif]-->
</head>

<body>

	<!-- Header -->

	<% include header %>

		<!-- End Header -->

		<!--====== Forgot Password Page ======-->

		<section class="contact section-padding">
			<div class="container">

				<div class="section-head spac">
					<h3>Forgot Password</h3>
				</div>

				<div class="wrapper">
					<div id="formContent">
						<!-- Tabs Titles -->

						<% if (message.length> 0) { %>
							<div class="alert alert-danger col-sm-12">
								<%= message %>
							</div>
							<% } %>

								<!-- Login Form -->
								<form class="mb-10" method="POST" action="/login">
									<input type="email" id="login" class="second" name="emailId"
										placeholder="Email Address" required>
									<input type="submit" class="fourth" value="Send">
								</form>
								<a class="forgot-password" href="/forgotpassword">Forgot Password</a>

								<!-- Remind Passowrd -->
								<!-- <div id="formFooter">
							<a class="underlineHover" href="#">Forgot Password?</a>
						</div> -->

					</div>
				</div>

			</div>
		</section>
		<!--====== End Login Page ======-->

		<!-- footer -->

		<% include footer %>

			<!-- End Footer -->

</body>

</html>